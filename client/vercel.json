{
    "version": 2,
    "builds": [
        {
            "src": "src/index.js",
            "use": "@vercel/node"
        },
        {
            "src": "public/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [{ "src": "/(.*)", "dest": "/index.html" }],
    "outputDirectory": "dist"
}
